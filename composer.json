{
    "name": "iksaku/jorgeglz.io",
    "type": "project",
    "description": "My personal website.",
    "keywords": [
        "framework",
        "laravel",
        "blog",
        "website"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "^1.6",
        "benjaminhirsch/nova-slug-field": "^1.2",
        "fideloper/proxy": "^4.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.6.2",
        "laravel/framework": "5.8.*",
        "laravel/nova": "^2.0",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "sausin/laravel-ovh": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "brainmaestro/composer-git-hooks": "^2.7",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.4",
        "lorisleiva/laravel-deployer": "^0.2.11",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-commit": "vendor/bin/php-cs-fixer fix",
            "pre-push": "vendor/bin/php-cs-fixer fix --dry-run",
            "post-merge": "composer update"
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:iksaku/nova.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "vendor/bin/cghooks update",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-install-cmd": "vendor/bin/cghooks add --ignore-lock"
    }
}
